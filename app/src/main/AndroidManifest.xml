<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.t01_alunocursos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.T01AlunoCursos">
        <activity android:name=".activities.ConsultaActivity"></activity>
        <activity
            android:name=".activities.CursosActivity"
            android:label="@string/title_activity_cursos"
            android:theme="@style/Theme.T01AlunoCursos.NoActionBar" />
        <activity
            android:name=".activities.AlunoActivity"
            android:label="@string/title_activity_alunos"
            android:theme="@style/Theme.T01AlunoCursos.NoActionBar" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>